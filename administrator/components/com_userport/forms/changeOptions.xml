<?xml version="1.0" encoding="UTF-8"?>
<form>
  <fields name="changeOptions">
    <fieldset name="addOptions">
      <field type="radio" name="add_password_handling_origin" default="use_import_value_else_random"
        label="COM_USERPORT_LABEL_OPTION_PASSWORD_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_PASSWORD_HANDLING_ORIGIN">
        <option value="use_import_value_else_override">COM_USERPORT_LABEL_ADD_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_import_value_else_random">COM_USERPORT_LABEL_ADD_OPTION_PASSWORD_HANDLING_ORIGIN_USE_IMPORT_VALUE_ELSE_RANDOM_VALUE</option>        
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>      
      <field type="radio" name="add_groups_handling_origin" default="use_import_value"
        label="COM_USERPORT_LABEL_OPTION_GROUPS_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_GROUPS_HANDLING_ORIGIN">
        <option value="use_import_value">COM_USERPORT_LABEL_ADD_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>
      <field type="radio" name="add_block_activate_handling_origin" default="use_import_value"
        label="COM_USERPORT_LABEL_OPTION_BLOCK_ACTIVATE_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_BLOCK_ACTIVATE_HANDLING_ORIGIN">
        <option value="use_import_value">COM_USERPORT_LABEL_ADD_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>
      <field type="radio" name="add_params_handling_origin" default="use_import_value"
        label="COM_USERPORT_LABEL_OPTION_PARAMS_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_PARAMS_HANDLING_ORIGIN">
        <option value="use_import_value">COM_USERPORT_LABEL_ADD_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>
    </fieldset>
    <fieldset name="updateOptions">
      <field type="radio" name="update_password_handling" default="0"
        label="COM_USERPORT_LABEL_UPDATE_OPTION_PASSWORD_HANDLING" description="COM_USERPORT_DESCRIPTION_UPDATE_OPTION_PASSWORD_HANDLING">
        <option value="0">JNO</option>
        <option value="1">JYES</option>
      </field>
      <field type="radio" name="update_password_handling_origin" default="use_import_value"
        label="COM_USERPORT_LABEL_OPTION_PASSWORD_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_PASSWORD_HANDLING_ORIGIN">
        <option value="use_import_value">COM_USERPORT_LABEL_UPDATE_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>      
      <field type="radio" name="update_groups_handling" default="1"
        label="COM_USERPORT_LABEL_UPDATE_OPTION_GROUPS_HANDLING" description="COM_USERPORT_DESCRIPTION_UPDATE_OPTION_GROUPS_HANDLING">
        <option value="0">JNO</option>
        <option value="merge">COM_USERPORT_LABEL_UPDATE_OPTION_GROUPS_HANDLING_MERGE</option>
        <option value="1">COM_USERPORT_LABEL_UPDATE_OPTION_GROUPS_HANDLING_REPLACE</option>
      </field> 
      <field type="radio" name="update_groups_handling_origin" default="use_import_value"
        label="COM_USERPORT_LABEL_OPTION_GROUPS_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_GROUPS_HANDLING_ORIGIN">
        <option value="use_import_value">COM_USERPORT_LABEL_UPDATE_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>
      <field type="radio" name="update_block_activate_handling" default="1"
        label="COM_USERPORT_LABEL_UPDATE_BLOCK_ACTIVATE_HANDLING" description="COM_USERPORT_DESCRIPTION_UPDATE_OPTION_BLOCK_ACTIVATE_HANDLING">
        <option value="0">JNO</option>
        <option value="1">JYES</option>
      </field>     
      <field type="radio" name="update_block_activate_handling_origin" default="use_import_value"
        label="COM_USERPORT_LABEL_OPTION_BLOCK_ACTIVATE_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_BLOCK_ACTIVATE_HANDLING_ORIGIN">
        <option value="use_import_value">COM_USERPORT_LABEL_UPDATE_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>
      <field type="radio" name="update_params_handling" default="merge"
        label="COM_USERPORT_LABEL_UPDATE_OPTION_PARAMS_HANDLING" description="COM_USERPORT_DESCRIPTION_UPDATE_OPTION_PARAMS_HANDLING">
        <option value="0">JNO</option>
        <option value="merge">COM_USERPORT_LABEL_OPTION_PARAMS_HANDLING_MERGE</option>
        <option value="1">COM_USERPORT_LABEL_OPTION_PARAMS_HANDLING_REPLACE</option>
      </field>     
      <field type="radio" name="update_params_handling_origin" default="use_import_value"
        label="COM_USERPORT_LABEL_OPTION_PARAMS_HANDLING_ORIGIN" description="COM_USERPORT_DESCRIPTION_OPTION_PARAMS_HANDLING_ORIGIN">
        <option value="use_import_value">COM_USERPORT_LABEL_UPDATE_OPTION_HANDLING_ORIGIN_USE_IMPORT_VALUE</option>
        <option value="use_override_value">COM_USERPORT_LABEL_OPTION_HANDLING_ORIGIN_USE_OVERRIDE_VALUE</option>
      </field>       
    </fieldset>    
    <fieldset name="addAndUpdateOptions">
      <field type="radio" name="passwords_are_encrypted" default="0"
        label="COM_USERPORT_LABEL_OPTION_PASSWORDS_ARE_ENCRYPTED" description="COM_USERPORT_DESCRIPTION_OPTION_PASSWORDS_ARE_ENCRYPTED">
        <option value="0">JNO</option>
        <option value="1">JYES</option>
      </field>
      <field type="text" name="password_override_value" size="80" maxLength="80"
        label="COM_USERPORT_LABEL_OPTION_PASSWORD_OVERRIDE_VALUE" description="COM_USERPORT_DESCRIPTION_OPTION_PASSWORD_OVERRIDE_VALUE" />
      <field type="usergroup" name="groups_override_value" default="2" multiple="true" size="10"
        label="COM_USERPORT_LABEL_OPTION_GROUPS_OVERRIDE_VALUE" description="COM_USERPORT_DESCRIPTION_OPTION_GROUPS_OVERRIDE_VALUE" />
      <field type="list" name="block_activate_override_value" default="default"
        label="COM_USERPORT_LABEL_OPTION_BLOCK_ACTIVATE_OVERRIDE_VALUE" description="COM_USERPORT_DESCRIPTION_OPTION_BLOCK_ACTIVATE_OVERRIDE_VALUE">
        <option value="no_yes">COM_USERPORT_LABEL_OPTION_BLOCK_ACTIVATE_OVERRIDE_VALUE_NO_YES</option>
        <option value="yes_yes">COM_USERPORT_LABEL_OPTION_BLOCK_ACTIVATE_OVERRIDE_VALUE_YES_YES</option>
        <option value="yes_no">COM_USERPORT_LABEL_OPTION_BLOCK_ACTIVATE_OVERRIDE_VALUE_YES_NO</option>
      </field>     
      <field type="text" name="params_override_value" size="80" maxLength="4096"
        label="COM_USERPORT_LABEL_OPTION_PARAMS_OVERRIDE_VALUE" description="COM_USERPORT_DESCRIPTION_OPTION_PARAMS_OVERRIDE_VALUE" />
    </fieldset>
    <fieldset name="genericOptions">
      <field type="spacer" name="help_dry_run"
        label="COM_USERPORT_HELP_DRY_RUN" />
      <field type="radio" name="dry_run" default="1"
        label="COM_USERPORT_LABEL_OPTION_DRY_RUN" description="COM_USERPORT_DESCRIPTION_OPTION_DRY_RUN">
        <option value="0">JNO</option>
        <option value="1">JYES</option>
      </field>
      <field type="email" name="dry_run_email" size="80" maxLength="255"
        label="COM_USERPORT_LABEL_OPTION_DRY_RUN_EMAIL" description="COM_USERPORT_DESCRIPTION_OPTION_DRY_RUN_EMAIL" />
      <field type="radio" name="notify_user" default="0"
        label="COM_USERPORT_LABEL_OPTION_NOTIFY_USER" description="COM_USERPORT_DESCRIPTION_OPTION_NOTIFY_USER">
        <option value="0">JNO</option>
        <option value="1">JYES</option>
      </field>
      <field type="text" name="email_subject" size="80" maxLength="255"
        label="COM_USERPORT_LABEL_OPTION_EMAIL_SUBJECT" description="COM_USERPORT_DESCRIPTION_OPTION_EMAIL_SUBJECT" />
      <field type="editor" name="email_body" rows="10" cols="120"
        label="COM_USERPORT_LABEL_OPTION_EMAIL_BODY" description="COM_USERPORT_DESCRIPTION_OPTION_EMAIL_BODY" />
      <field type="spacer" name="help_email_1"
        label="COM_USERPORT_HELP_EMAIL_1" />
      <field type="spacer" name="help_email_2"
        label="COM_USERPORT_HELP_EMAIL_2" />
      <field type="spacer" name="help_email_3"
        label="COM_USERPORT_HELP_EMAIL_3" />
      <field type="spacer" name="help_email_4"
        label="COM_USERPORT_HELP_EMAIL_4" />
      <field type="spacer" name="help_email_5"
        label="COM_USERPORT_HELP_EMAIL_5" />
      <field type="spacer" name="help_email_6"
        label="COM_USERPORT_HELP_EMAIL_6" />        
    </fieldset>
  </fields>
</form>
